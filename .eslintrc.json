{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": [
      "eslint:recommended",
      "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "settings": {
      "import/resolver": {
        "node": {
          "extensions": [".js", ".ts"]
        }
      }
    },
    "rules": {
      "no-console": "off",
      "indent": ["error", 2],
      "quotes": ["error", "single"],
      "linebreak-style": "off",
      "import/extensions": "off"
    }
}
